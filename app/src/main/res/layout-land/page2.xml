<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7EB5306E"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="94dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:text="Connaissez-vous votre indice de masse corporelle (IMC) ?"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="601dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="115dp"
        android:layout_height="96dp"
        android:layout_marginEnd="84dp"
        app:layout_constraintEnd_toStartOf="@+id/coeur"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/coeur"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/coeur"
        android:layout_width="268dp"
        android:layout_height="65dp"
        android:text="@string/mon_coeur"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/prblmcardiaque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:text="@string/avez_vous_un_probl_me_cardiaque"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/prblmcolesterol"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/prblmcolesterol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/avez_vous_un_probl_me_de_cholest_rol"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="137dp" />

    <TextView
        android:id="@+id/diabetique"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/etes_vous_diab_tique"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/spinner4"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="183dp" />

    <TextView
        android:id="@+id/hypertension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/avez_vous_de_l_hypertension"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="230dp" />

    <TextView
        android:id="@+id/AVC"
        android:layout_width="537dp"
        android:layout_height="64dp"
        android:layout_marginEnd="23dp"
        android:text="@string/avez_vous_un_parent_du_premier_degr_p_re_m_re_fr_re_ou_s_ur_qui_a_eu_des_probl_mes_cardiaques_infarctus_avc_mort_subite_avant_55_ans_pour_un_homme_ou_avant_65_ans_chez_une_femme"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/spinner6"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="278dp" />

    <TextView
        android:id="@+id/IMC"
        android:layout_width="274dp"
        android:layout_height="46dp"
        android:layout_marginEnd="23dp"
        android:text="Connaissez-vous votre indice de masse corporelle (IMC) ?"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/spinner7"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="601dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="112dp"
        android:layout_height="29dp"
        android:entries="@array/Spiner1_page2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.677"
        app:layout_constraintStart_toEndOf="@+id/prblmcardiaque"
        tools:layout_editor_absoluteY="82dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="114dp"
        android:layout_height="34dp"
        android:entries="@array/Spiner1_page2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.61"
        app:layout_constraintStart_toEndOf="@+id/prblmcolesterol"
        tools:layout_editor_absoluteY="130dp" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="112dp"
        android:layout_height="30dp"
        android:layout_marginEnd="48dp"
        android:entries="@array/Spiner1_page2"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="183dp" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="102dp"
        android:layout_height="27dp"
        android:layout_marginStart="172dp"
        android:layout_marginBottom="33dp"
        android:entries="@array/Spiner1_page2"
        app:layout_constraintBottom_toTopOf="@+id/spinner6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.605"
        app:layout_constraintStart_toEndOf="@+id/hypertension"
        app:layout_constraintTop_toTopOf="@+id/hypertension"
        app:layout_constraintVertical_bias="0.0" />

    <Spinner
        android:id="@+id/spinner6"
        android:layout_width="104dp"
        android:layout_height="37dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="44dp"
        android:entries="@array/Spiner2_page2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="parent" />

    <Spinner
        android:id="@+id/spinner7"
        android:layout_width="105dp"
        android:layout_height="29dp"
        android:layout_marginEnd="16dp"
        android:entries="@array/Spiner2_page2"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="601dp" />

    <Spinner
        android:id="@+id/spinner8"
        android:layout_width="102dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:entries="@array/Spiner1_page2"
        app:layout_constraintStart_toEndOf="@+id/AVC"
        tools:layout_editor_absoluteY="290dp" />

    <TextView
        android:id="@+id/IMC"
        android:layout_width="561dp"
        android:layout_height="33dp"
        android:text="@string/connaissez_vous_votre_indice_de_masse_corporelle_imc"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="362dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="103dp"
        android:layout_height="26dp"
        android:entries="@array/Spiner2_page2"
        tools:layout_editor_absoluteX="596dp"
        tools:layout_editor_absoluteY="362dp" />

</androidx.constraintlayout.widget.ConstraintLayout>