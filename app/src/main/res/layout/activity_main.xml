<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/Pageacceuil_Image"
        android:layout_width="395dp"
        android:layout_height="270dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/comment_va_votre_coeur"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/Pageacceuil_NOM"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:text="Nom :"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="367dp" />

    <Button
        android:id="@+id/Pageacceuil_button_test"
        android:layout_width="175dp"
        android:layout_height="85dp"
        android:text="Je me test"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="561dp" />

    <EditText
        android:id="@+id/Pageacceuil_Champsnom"
        android:layout_width="378dp"
        android:layout_height="64dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="417dp" />

</androidx.constraintlayout.widget.ConstraintLayout>